{
  "printWidth": 120,
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2,
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": false,
  "overrides": [
    {
      "files": ".yaml",
      "options": { "parser": "yaml" }
    },
    {
      "files": ".yml",
      "options": { "parser": "yaml" }
    },
    {
      "files": ".vue",
      "options": { "parser": "vue" }
    },
    {
      "files": ".md",
      "options": { "parser": "markdown" }
    }
  ]
}
